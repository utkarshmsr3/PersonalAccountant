	<section id="signup" class="text-center">
	<div class="container">
		<div class="row">
			<div class="col-md-12 wow fadeInDown" data-wow-delay="2000">
				<h3 style="font-size: 40px">Sign Up</h3>
			</div>
			<div class="col-md-2"></div>
			<div class="col-md-8">
				<p style="font-size: 25px">Register yourself to get the real
					experience of comfort.</p>
			</div>
			<div class="col-md-2"></div>
			<script type="text/javascript">
				function validatePassword() {
					var p = document.getElementById("signUpPassword").value;
					var cp = document.getElementById("signUpConfirmPassword").value;
					if (cp == p) {
						return true;
					}
					document.getElementById("signUpConfirmPassword")
							.setCustomValidity("Password Mismatch");
					return false;
				}

				function validate() {
					return validatePassword();
				}
			</script>
			<br> ${requestScope.Notification}
			<form action="/MyProject/SignUpServlet" method="post"
				onsubmit="return validate();">
				<div class="col-md-6 wow fadeInLeft" data-wow-delay="2000">
					<input type="text" name="signUpFirstName" id="signUpFirstName"
						placeholder="First Name" class="form-control" required>
				</div>
				<div class="col-md-6 wow fadeInLeft" data-wow-delay="2000">
					<input type="text" name="signUpLastName" id="signUpLastName"
						placeholder="Last Name" class="form-control">
				</div>
				<div class="col-md-12 wow fadeInLeft" data-wow-delay="2000">
					<input type="text" name="signUpUsername" id="signUpUsername"
						placeholder="Userame" class="form-control" required>
				</div>
				<div class="col-md-6 wow fadeInRight" data-wow-delay="2000">
					<input type="password" id="signUpPassword" name="signUpPassword"
						placeholder="Password" class="form-control" required>
				</div>
				<div class="col-md-6 wow fadeInRight" data-wow-delay="2000">
					<input type="password" id="signUpConfirmPassword"
						name="signUpConfirmPassword" placeholder="Confirm Password"
						oninput="setCustomValidity('')" class="form-control" required>
				</div>
				<div class="col-md-6 wow fadeInDown" data-wow-delay="2000">
					<input type="email" id="signUpEmail" name="signUpEmail"
						placeholder="Email" class="form-control" required>
				</div>

				<div class="col-md-6 wow fadeInDown" data-wow-delay="2000">
					<input type="text" id="signUpMobileNo" name="signUpMobileNo"
						pattern="[0-9]{10}" placeholder="Mobile No." class="form-control"
						oninput="setCustomValidity('')"
						oninvalid="this.setCustomValidity('Please enter a valid mobile number')"
						required>
				</div>
				<div class="col-md-12 wow fadeInDown" data-wow-delay="2000">
					<textarea id="signUpAddress" name="signUpAddress"
						placeholder="Address" class="form-control" required></textarea>
				</div>
				<div class="col-md-3"></div>
				<div class="col-md-3 wow fadeInUp" data-wow-delay="2000">
					<input type="submit" value="Register" class="form-control">
				</div>
				<div class="col-md-3 wow fadeInUp" data-wow-delay="2000">
					<input type="reset" value="Reset" class="form-control">
				</div>
				<div class="col-md-3"></div>
			</form>
		</div>
